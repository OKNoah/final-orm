    "eslint": "^4.3.0",
    "jest": "^20.0.4",
    "moment": "^2.18.1"
  },
  "dependencies": {
    "arangojs": "^5.7.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4"
